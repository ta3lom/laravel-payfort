{
  "name": "ta3lom/laravel-payfort",
  "description": "Laravel package for Payfort payment gateway from moeen-basra",
  "type": "library",
  "require": {
    "php": "^7.2|>= 8.0",
    "moneyphp/money": "^3.2",
    "guzzlehttp/guzzle": "^6.0|^7.0",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Moeen Basra",
      "email": "m.basra@live.com"
    },
    {
      "name" : "Islam"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.0.0",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "MoeenBasra\\Payfort\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MoeenBasra\\Payfort\\PayfortServiceProvider"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
